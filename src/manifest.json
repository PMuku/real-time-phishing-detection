{
    "name": "My Anti-Phishing Extension",
    "version": "1.0",
    "manifest_version": 3,
    "description": "A real-time anti-phishing browser extension.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background/serviceWorker.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content/content.js"]
        }
    ]
    
}